  "repository": "justjavac/jest-badges",
  "bugs": "https://github.com/justjavac/jest-badges/issues",
  "author": "justjavac <justjavac@gmail.com> (https://justjavac.com)",
  "files": [
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "dependencies": {
    "istanbul-lib-report": "^3.0.0"
  },
  "devDependencies": {
    "@types/istanbul-lib-report": "^3.0.0",
    "typescript": "^4.2.3"
  }
}
